{
  "name": "h5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "protoc": "node generate.js",
    "wasm:build": "wasm-pack build ./wasm --target web",
    "wasm:link": "npm link ./wasm/pkg",
    "dev": "vite --mode dev",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build --mode prod",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.8.2",
    "@protobuf-ts/plugin": "^2.8.2",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "mitt": "^3.0.0",
    "pinia": "^2.0.23",
    "spark-md5": "^3.0.1",
    "tinymce": "^6.2.0",
    "vant": "^3.6.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rollup/plugin-dynamic-import-vars": "^2.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^21.0.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "start-server-and-test": "^1.15.2",
    "terser": "^5.16.1",
    "typescript": "^5.0.2",
    "unplugin-vue-components": "0.22.12",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-wasm": "^3.1.1",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.0.24",
    "weixin-js-sdk": "^1.6.0"
  }
}
