{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "protoc": "node generate.js",
    "wasm:build": "wasm-pack build ./wasm --target web",
    "wasm:link": "npm link ./wasm/pkg",
    "pc:dev": "vite --config pc.vite.config.ts --mode dev",
    "pc:build": "run-p type-check pc:build-only",
    "pc:preview": "vite preview --config pc.vite.config.ts --port 4173",
    "pc:test:unit": "vitest --environment jsdom",
    "pc:test:e2e": "start-server-and-test pc:preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "pc:test:e2e:ci": "start-server-and-test pc:preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "pc:build-only": "vite build --config pc.vite.config.ts --mode prod",
    "h5:dev": "vite --config h5.vite.config.ts --mode dev",
    "h5:build": "run-p type-check h5:build-only",
    "h5:preview": "vite preview --config h5.vite.config.ts --port 4173",
    "h5:test:unit": "vitest --environment jsdom",
    "h5:test:e2e": "start-server-and-test h5:preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "h5:test:e2e:ci": "start-server-and-test h5:preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "h5:build-only": "vite build --config h5.vite.config.ts --mode prod",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@protobuf-ts/grpcweb-transport": "^2.9.0",
    "@protobuf-ts/plugin": "^2.9.0",
    "ant-design-vue": "^3.2.17",
    "axios": "^1.4.0",
    "dayjs": "^1.11.6",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "markdown-it": "^13.0.1",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.35",
    "push.js": "^1.0.12",
    "spark-md5": "^3.0.1",
    "tinymce": "^6.4.2",
    "vant": "^4.2.0",
    "vditor": "^3.9.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.1.5",
    "@iconify-json/logos": "^1.1.25",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.1",
    "@unocss/preset-attributify": "^0.51.8",
    "@unocss/preset-icons": "^0.51.8",
    "@unocss/preset-uno": "^0.51.8",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@volar-plugins/vetur": "latest",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "cypress": "^12.11.0",
    "eslint": "^8.31.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^21.1.1",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "terser": "^5.16.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.8",
    "unplugin-vue-components": "0.24.1",
    "vite": "^4.3.3",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-wasm": "^3.2.2",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.6.0",
    "weixin-js-sdk": "^1.6.0"
  }
}
